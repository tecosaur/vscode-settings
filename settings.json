{
    "editor.fontLigatures": true,
    "window.zoomLevel": 1,
    "editor.fontSize": 16,
    "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', consolas, monospace",
    "workbench.iconTheme": "material-icon-theme",
    "window.titleBarStyle": "custom",
    "workbench.statusBar.feedback.visible": false,
    // ————————————————————————————
    // Editor Feel
    // ————————————————————————————
    "breadcrumbs.enabled": true,
    "window.enableMenuBarMnemonics": false,
    // cursor
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    // suggestions / snippets
    "editor.snippetSuggestions": "top",
    "editor.wordBasedSuggestions": true,
    "editor.tabCompletion": "onlySnippets",
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.quickSuggestionsDelay": 0,
    // other
    "editor.formatOnSave": true,
    "editor.emptySelectionClipboard": false,
    "editor.wordSeparators": "`-~!@#$%^&*()=+[\\{]\\}\\|;:'\",.<>/?",
    "editor.renderWhitespace": "selection",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "files.exclude": {
        "**/._*": true,
        // Latex files
        "**/*.synctex.gz": true,
        "**/*.fls": true,
        "**/*.fdb_latexmk": true,
        "**/*.aux": true,
        "**/*.ptc": true,
        "**/_minted-*": true,
        "**/*.dvi": true,
        "**/*.toc": true,
        // Latex Glossary Files
        "**/*.glg": true,
        "**/*.glo": true,
        "**/*.gls": true,
        "**/*.glsdefs": true,
        "**/*.ist": true,
        "**/*.acn": true,
        "**/*.acr": true,
        "**/*.alg": true,
        // LaTeX generated bibliography files
        "**/*.bbl": true,
        "**/*.bcf": true,
        "**/*.blg": true,
        "**/*.run.xml": true,
        // SageTex Files
        "**/*.sagetex.sage": true,
        "**/*.sagetex.sage.py": true,
        "**/*.sagetex.scmd": true,
        "**/*.sagetex.sout": true,
        // Other filles
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.associations": {
        "*.latex": "latex"
    },
    "files.insertFinalNewline": true,
    "keyboard.dispatch": "keyCode", // make capsLock to backspace key remap work in vs code
    // ————————————————————————————
    // File-type specific
    // ————————————————————————————
    "[latex]": {
        "editor.wordWrap": "on",
        "editor.fontFamily": "'IBM Plex Sans', Verdana", // or: Comic Sans MS
        "editor.fontSize": 18,
        "editor.fontWeight": "400",
        "editor.fontLigatures": true,
        "editor.letterSpacing": 0.15,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.rulers": [65]
    },
    "bracket-pair-colorizer-2.excludedLanguages": ["latex"],
    "[markdown]": {
        "editor.wordWrap": "on"
        // "editor.fontFamily": "IBM Plex Sans", // or: Verdana, Comic Sans MS
        // "editor.fontFamily": "IBM Plex Sans" // or: Verdana, Comic Sans MS
        // "editor.letterSpacing": 0.15
    },
    "[javascript]": {
        "editor.suggestSelection": "first",
        "editor.rulers": [140],
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.suggestSelection": "first",
        "editor.rulers": [140],
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.rulers": [140]
    },
    "[python]": {
        "editor.suggestSelection": "first",
        "editor.rulers": [88]
    },
