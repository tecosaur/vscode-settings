        {
            "name": "bibtex",
            "command": "bibtex",
            "args": ["%DOCFILE%"]
        },
        {
            "name": "sage",
            "command": "sage",
            "args": ["%DOC%.sagetex.sage"]
        }
    ],
    "latex-workshop.latex.recipes": [
