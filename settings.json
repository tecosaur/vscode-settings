    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": ["-synctex=1", "-interaction=nonstopmode", "-shell-escape", "-file-line-error", "-pdf", "%DOC%"]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": ["-synctex=1", "-interaction=nonstopmode", "-shell-escape", "-file-line-error", "%DOC%"]
        },
        {
            "name": "precompile preamble",
            "command": "pdftex",
            "args": [
                "-ini",
                "-interaction=nonstopmode",
                "-shell-escape",
                "-file-line-error",
                "-jobname=\"preamble\"",
                "\"&pdflatex\"",
                "mylatexformat.ltx",
                "\"%DOC%.tex\""
            ]
        },
        {
            "name": "reference precompiled preamble",
            "command": "sed",
            "args": ["-i", "1s/^/%\\&preamble\\n/", "%DOC%.tex"]
        },
        {
            "name": "remove any reference precompiled preamble",
            "command": "sed",
            "args": ["-i", ":begin;$!N;s/%\\&preamble\\n//;tbegin;P;D", "%DOC%.tex"]
        },
        {
            "name": "biber",
            "command": "biber",
            "args": ["%DOCFILE%"]
        },
        {
            "name": "sage",
            "command": "sage",
            "args": ["%DOC%.sagetex.sage"]
        },
        {
            "name": "compress",
            "command": "/home/tec/Documents/pdf-postprocess.sh",
            "args": ["%DOC%.pdf"]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": " 🔃   latexmk",
            "tools": ["latexmk"]//, "compress"]
        },
        {
            "name": "$(link)   precompile preamble",
            "tools": [
                "precompile preamble",
                "remove any reference precompiled preamble",
                "reference precompiled preamble",
                "latexmk"
            ]
        },
        {
            "name": " $(fold)   compress",
            "tools": ["compress"]
        },
        {
            "name": " $(file)   pdflatex",
            "tools": ["pdflatex"]
        },
        {
            "name": " $(repo)    pdflatex ➞ biber ➞ pdflatex × 2",
            "tools": ["pdflatex", "biber", "pdflatex", "pdflatex"]
        },
        {
            "name": " $(graph)  pdflatex ➞ ⬡ sage ➞ pdflatex",
            "tools": ["pdflatex", "sage", "pdflatex"]
        }
    ],
