    "highlight.regexes": {
        "((?:<!— *)?(?:#|//|/\\*+|<!—|%) *TODO:?)(?!\\w)((?: +[^\n@]+?)(?= *(?:[^:]//|/\\*+|<!—|@))|(?: +[^@\n]+)?)": [
