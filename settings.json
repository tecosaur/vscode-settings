    "latex-workshop.chktex.args.active": ["-wall", "-n22", "-n30", "-e16", "-q", "-n1", "-n24"],
    "latex-workshop.chktex.args.root": ["-wall", "-n22", "-n30", "-e16", "-q", "-n1", "-n24"],
    "latex-workshop.hover.preview.cursor.symbol": "\\!|\\!",
    "latex-workshop.hover.preview.cursor.color": "gray",
    "latex-workshop.hover.preview.cursor.enabled": true,
    "latex-workshop.view.pdf.invert": 0.0,
    "latex-workshop.progress.runIconType": "Solid Circled",
