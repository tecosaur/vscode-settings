                "color": "#1f1f1f",
                "fontWeight": "bold"
            },
            {
                "backgroundColor": "#444",
                "color": "#ddd"
            }
        ],
        "((?:<!— *)?(?:#|//|/\\*+|<!—|%) *MISSING:?)(?!\\w)((?: +[^\n@]+?)(?= *(?:[^:]//|/\\*+|<!—|@))|(?: +[^@\n]+)?)": [
            {
                "overviewRulerColor": "#88C",
                "backgroundColor": "#88C",
                "color": "#1f1f1f",
                "fontWeight": "bold"
            },
            {
                "backgroundColor": "#447",
                "color": "#ddd"
            }
        ],
        "((?:<!— *)?(?:#|//|/\\*+|<!—|%) *ADD\\s?:)(?!\\w)((?: +[^\n@]+?)(?= *(?:[^:]//|/\\*+|<!—|@))|(?: +[^@\n]+)?)": [
            {
                "overviewRulerColor": "#8C8",
                "backgroundColor": "#8C8",
                "color": "#1f1f1f",
                "fontWeight": "bold"
            },
            {
                "backgroundColor": "#474",
                "color": "#ddd"
            }
        ],
        "(// )(@\\w+)": [
            {},
            {
                "color": "#4de0ff"
            }
        ],
        // \todo[opt[[[[]]]]]{val}
        "(\\\\todo\\[?[^\\]\\{\\}]*\\]?)(\\{.*\\})": [
            {
                "overviewRulerColor": "#2D3A3E",
                "backgroundColor": "#2D3A3E",
                "color": "#F6FCFA",
                "fontWeight": "bold"
            },
            {
                "backgroundColor": "#2D7A7E",
                "color": "#ddd"
            }
        ]
    },
