    "todo.embedded.regex": "(?:<!â€” *)?(?:#|//|/\\*+|<!â€”|â€”|%) *(TODO|FIXME|FIX|BUG|UGLY|HACK|NOTE|IDEA|REVIEW|DEBUG|OPTIMIZE|MISSING|ADD)(?:\\([^(]+\\))?:?(?!\\w)(?: *â€”>| *\\*/|(?= *(?:[^:]//|/\\*+|<!â€”|@|â€”))|((?: +[^\n@]*?)(?= *(?:[^:]//|/\\*+|<!â€”|@|â€”))|(?: +[^@\n]+)?))",
    "todo.embedded.providers.rg.regex": "(?:#|//|/\\*+|<!â€”|â€”|%) *(TODO|FIXME|FIX|BUG|UGLY|HACK|NOTE|IDEA|REVIEW|DEBUG|OPTIMIZE|MISSING|ADD)",
    "todo.embedded.providers.ag.regex": "(?:#|//|/\\*+|<!â€”|â€”|%) *(TODO|FIXME|FIX|BUG|UGLY|HACK|NOTE|IDEA|REVIEW|DEBUG|OPTIMIZE|MISSING|ADD)",
    "latex-workshop.chktex.enabled": true,
