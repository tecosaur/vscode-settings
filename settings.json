    "highlight.regexes": {
        "((?:<!â€” *)?(?:#|//|/\\*+|<!â€”|%) *TODO:?)(?!\\w)((?: +[^\n@]+?)(?= *(?:[^:]//|/\\*+|<!â€”|@))|(?: +[^@\n]+)?)": [
