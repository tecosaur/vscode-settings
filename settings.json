{
    "editor.fontLigatures": true,
    "window.zoomLevel": 1,
    "editor.fontSize": 16,
    "editor.fontFamily": "Fira Code",
    "workbench.colorTheme": "Monokai Pro",
    "workbench.iconTheme": "Monokai Pro Icons",
    "window.titleBarStyle": "custom",
    "workbench.statusBar.feedback.visible": false,

