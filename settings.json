        "((?:<!— *)?(?:#|//|/\\*+|<!—|%) *NOTE:?)(?!\\w)((?: +[^\n@]+?)(?= *(?:[^:]//|/\\*+|<!—|@))|(?: +[^@\n]+)?)": [
            {
                "overviewRulerColor": "#888",
